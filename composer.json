{
    "name": "prezent/ink-bundle",
    "type": "library",
    "description": "Easy responsive e-mails using Foundation and Inky",
    "keywords": ["email", "responsive", "twig", "foundation", "inky"],
    "homepage": "http://www.prezent.nl",
    "require": {
        "php": "^7.0|^8.0",
        "lib-libxml": ">=2.7.8",
        "prezent/inky": "^0.1",
        "pelago/emogrifier": "^3.0",
        "symfony/framework-bundle": "^3.3|^4.0|^5.0",
        "swiftmailer/swiftmailer": "^5.4|^6.0",
        "symfony/twig-bundle": "^3.0|^4.0|^5.0"
    },
    "require-dev": {
        "symfony/css-selector": "^3.3|^4.0|^5.0",
        "symfony/browser-kit": "^3.3|^4.0|^5.0",
        "symfony/phpunit-bridge": "^3.3|^4.0|^5.0",
        "symfony/web-profiler-bundle": "^3.3|^4.0|^5.0",
        "symfony/swiftmailer-bundle": "^2.3|^3.0",
        "symfony/yaml": "^3.3|^4.0|^5.0",
        "twig/twig": "^2.0|^3.0",
        "phpunit/phpunit": "^9.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sander Marechal",
            "email": "sander@prezent.nl"
        },
        {
            "name": "Prezent Internet B.V.",
            "email": "github@prezent.nl"
        }
    ],
    "autoload": {
        "psr-4": { "Prezent\\InkBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Prezent\\InkBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
